<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
    version="2.4">

    <display-name>Hello, World Application</display-name>
    <description>
	This is a simple web application with a source code organization
	based on the recommendations of the Application Developer's Guide.
    </description>

    <resource-ref>
        <res-ref-name>jdbc/OssaplDS</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>

    <servlet>
        <servlet-name>SetCacheServlet</servlet-name>
        <servlet-class>servlet.SetCache</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCacheServlet</servlet-name>
        <servlet-class>servlet.GetCache</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PutMQServlet</servlet-name>
        <servlet-class>servlet.PutMq</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetMQServlet</servlet-name>
        <servlet-class>servlet.GetMq</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertDBServlet</servlet-name>
        <servlet-class>servlet.InsertDb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SelectDBServlet</servlet-name>
        <servlet-class>servlet.SelectDb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteDBServlet</servlet-name>
        <servlet-class>servlet.DeleteDb</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SetCacheServlet</servlet-name>
        <url-pattern>/SetCache</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCacheServlet</servlet-name>
        <url-pattern>/GetCache</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PutMQServlet</servlet-name>
        <url-pattern>/PutMQ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetMQServlet</servlet-name>
        <url-pattern>/GetMQ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertDBServlet</servlet-name>
        <url-pattern>/InsertDB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SelectDBServlet</servlet-name>
        <url-pattern>/SelectDB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteDBServlet</servlet-name>
        <url-pattern>/DeleteDB</url-pattern>
    </servlet-mapping>
</web-app>
